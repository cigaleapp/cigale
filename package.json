		"format": "prettier --write .",
		"lint": "npx oxlint && npx eslint",
		"lintfix": "npx oxlint --fix && npx eslint --fix",
		"commit": "gitmoji -c",
		"test": "vitest --coverage.enabled true",
		"test:ui": "vitest --ui",
		"test:watch": "vitest"
	},
	"lint-staged": {
		"*!(.svelte).{js,ts}": [
			"oxlint --fix"
		],
		"*.{svelte,svelte.{js,ts}}": [
			"oxlint --fix",
			"eslint --fix"
		],
		"*": [
			"prettier --write --ignore-unknown"

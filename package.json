{
	"name": "cigale",
	"private": true,
	"description": "Classification Intelligente et Gestion des Arthropodes et de L'Entomofaune",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"prebuild": "node scripts/generate-json-schemas.js",
		"preview": "vite preview",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint .",
		"lintfix": "eslint --fix .",
		"commit": "gitmoji -c",
		"postinstall": "merge-drivers install"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": [
			"eslint --fix"
		],
		"*": [
			"prettier --write --ignore-unknown"
		]
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.8",
		"@eslint/js": "^9.22.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.1.0",
		"execa": "^9.5.2",
		"gitmoji-cli": "^9.5.0",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"jsdom": "^26.0.0",
		"lint-staged": "^15.5.0",
		"mdsvex": "^0.12.3",
		"merge-drivers": "^1.0.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.25.3",
		"svelte-check": "^4.1.5",
		"turndown": "^7.2.0",
		"typescript": "^5.8.2",
		"vite": "^6.2.2",
		"vite-plugin-cross-origin-isolation": "^0.1.6"
	},
	"dependencies": {
		"@iconify-json/ph": "^1.2.2",
		"@tensorflow-models/mobilenet": "^2.1.1",
		"@tensorflow/tfjs": "^4.22.0",
		"arktype": "^2.1.12",
		"date-fns": "^4.1.0",
		"deep-equal": "^2.2.3",
		"dragselect": "^3.1.1",
		"exif-parser": "^0.1.12",
		"fflate": "^0.8.2",
		"fuse.js": "^7.1.0",
		"handlebars": "^4.7.8",
		"idb": "^8.0.2",
		"jimp": "^1.6.0",
		"marked": "^15.0.7",
		"onnxruntime-web": "^1.22.0-dev.20250310-fe7634eb6f",
		"pica-gpu": "^0.2.0",
		"sveltejs-tippy": "^3.0.0",
		"tinykeys": "^3.0.0",
		"unplugin-icons": "^22.1.0",
		"yaml": "^2.7.0"
	},
	"volta": {
		"node": "22.14.0",
		"npm": "11.2.0"
	}
}

{
	"name": "cigale",
	"private": true,
	"description": "Classification Intelligente et Gestion des Arthropodes et de L'Entomofaune",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"prebuild": "node scripts/generate-json-schemas.js",
		"preview": "vite preview",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint .",
		"lintfix": "eslint --fix .",
		"commit": "gitmoji -c",
		"postinstall": "merge-drivers install",
		"test": "vitest --coverage.enabled true",
		"test:ui": "vitest --ui",
		"test:watch": "vitest"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": [
			"eslint --fix"
		],
		"*": [
			"prettier --write --ignore-unknown"
		]
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.26.0",
		"@playwright/test": "^1.52.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.8",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/piexifjs": "^1.0.0",
		"@vitest/coverage-v8": "^3.1.2",
		"@vitest/ui": "^3.1.2",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^3.5.1",
		"execa": "^9.5.2",
		"extract-zip": "^2.0.1",
		"fake-indexeddb": "^6.0.0",
		"get-current-line": "^7.4.0",
		"gitmoji-cli": "^9.6.0",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"jsdom": "^26.1.0",
		"lint-staged": "^15.5.1",
		"mdsvex": "^0.12.5",
		"merge-drivers": "^1.0.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.28.2",
		"svelte-check": "^4.1.6",
		"turndown": "^7.2.0",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vite-plugin-cross-origin-isolation": "^0.1.6",
		"vitest": "^3.1.2"
	},
	"dependencies": {
		"@iconify-json/ph": "^1.2.2",
		"@tensorflow-models/mobilenet": "^2.1.1",
		"@tensorflow/tfjs": "^4.22.0",
		"arktype": "^2.1.20",
		"bits-ui": "^1.3.19",
		"date-fns": "^4.1.0",
		"deep-equal": "^2.2.3",
		"dragselect": "^3.1.1",
		"exif-parser": "^0.1.12",
		"fflate": "^0.8.2",
		"fuse.js": "^7.1.0",
		"handlebars": "^4.7.8",
		"idb": "^8.0.2",
		"jimp": "^1.6.0",
		"marked": "^15.0.11",
		"onnxruntime-web": "^1.22.0-dev.20250310-fe7634eb6f",
		"pica-gpu": "^0.2.0",
		"piexifjs": "^1.0.6",
		"sveltejs-tippy": "^3.0.0",
		"tinykeys": "^3.0.0",
		"unplugin-icons": "^22.1.0",
		"xss": "^1.0.15",
		"yaml": "^2.7.1"
	},
	"volta": {
		"node": "22.15.0",
		"npm": "11.3.0"
	}
}

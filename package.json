	"scripts": {
		"dev": "vite dev",
		"dev:native": "electron-forge start",
		"build": "vite build",
		"prebuild": "bun run scripts/download-google-fonts.js && bun run scripts/generate-json-schemas.js",
		"preview": "vite preview",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"check:clutter": "svelte-kit sync && bunx @inlang/paraglide-js compile --outdir src/lib/paraglide && bunx knip",
		"format": "prettier --write .",
		"lint": "bunx oxlint && bunx eslint",
		"lintfix": "bunx oxlint --fix && bunx eslint --fix",
		"commit": "gitmoji -c",
		"test": "vitest --coverage.enabled true",
		"test:ui": "vitest --ui",
		"test:watch": "vitest"
	},
	"lint-staged": {
		"*!(.svelte).{js,ts}": [
			"oxlint --fix"
		],
		"*.{svelte,svelte.{js,ts}}": [
			"oxlint --fix",
			"eslint --fix"
		],
		"*": [
			"prettier --write --ignore-unknown"
		]
	},
